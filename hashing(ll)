#include <stdio.h>
#include<stdlib.h>
#define size 10
struct node
{
    int data;
    struct node *next;
}*newnode,*temp,*head[size]={NULL};
void create()
{
    int hash,key,ch;
    do{
    printf("enter value:");
    scanf("%d",&key);
    hash=key%size;
    newnode=(struct node*)malloc(sizeof(struct node));
    newnode->data=key;
    newnode->next=NULL;
    if(head[key]==NULL)
    {
        head[key]=newnode;
    }
    else
    {
        temp=head[key];
        while(temp->next!=NULL)
        {
            temp=temp->next;
        }
        temp->next=newnode;
    }
    printf("enter 1 to continue");
    fflush(stdin);
    scanf("%d",&ch);
    }while(ch==1);
}
void display()
{
    int i;
    for(i=0;i<size;i++)
    {
        if(head[i]!=NULL)
        {
            for(temp=head[i];temp->next!=NULL;temp=temp->next)
            {
                printf("%d",temp->data);
            }
        }
    }
}
int main()
{
    int ch;
    printf("enter u r choice\n");
    printf("1-create\n2-display");
    scanf("%d",&ch);
    while(ch<3)
    {
    switch(ch)
    { case 1:
            create();
            break;
     case 2:
            display();
            break;
    }
    }
}
